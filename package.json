{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test": "vitest",
    "test:ci": "vitest --reporter=json --outputFile=.github/reports/test-results.json --run",
    "dev:fullstack": "npm run dev",
    "start": "npm run preview -- --host",
    "generate:photos": "node scripts/generatePhotoPairs.mjs",
    "upload:photos": "node scripts/uploadPhotos.mjs",
    "generate:bodies": "VITE_CHAT_API_URL=https://toharper.dad node scripts/generatePhotoBodies.mjs",
    "refresh:bodies": "VITE_CHAT_API_URL=https://toharper.dad FORCE_REFRESH_BODIES=true node scripts/generatePhotoBodies.mjs",
    "cache:letter-chunks": "node scripts/cacheLetterChunks.mjs",
    "cache:letter-chunks:wrangler": "node scripts/cacheLetterChunksWrangler.mjs",
    "migrate:d1": "node scripts/migrateD1.mjs",
    "migrate:d1:local": "node scripts/migrateD1.mjs --local",
    "datalake:setup": "node scripts/setup-datalake.mjs",
    "datalake:deploy": "cd workers && npx wrangler deploy",
    "datalake:trigger": "cd workers && npx wrangler trigger scheduled"
  },
  "dependencies": {
    "@langchain/core": "^0.2.16",
    "@langchain/openai": "^0.0.18",
    "canvas-confetti": "^1.9.3",
    "chokidar": "^3.5.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^11.5.4",
    "gsap": "^3.12.5",
    "langchain": "^0.2.20",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.6",
    "react-use-gesture": "^9.1.3",
    "sharp": "^0.33.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.2.4",
    "vitest": "^1.0.4",
    "wrangler": "^4.50.0"
  }
}
